{
  "name": "@jsdevtools/static-mock-data",
  "version": "4.0.2",
  "description": "Static mock data for sample apps, demos, and POCs",
  "keywords": [
    "data",
    "mock",
    "sample",
    "samples",
    "users",
    "mock data",
    "sample data",
    "mock user data",
    "sample user data"
  ],
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "license": "(MIT OR CC-BY-NC-SA-4.0)",
  "homepage": "https://jstools.dev/static-mock-data",
  "repository": {
    "type": "git",
    "url": "https://github.com/JS-DevTools/static-mock-data.git"
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "browser": {
    "./cjs/isomorphic.node.js": "./cjs/isomorphic.browser.js",
    "./esm/isomorphic.node.js": "./esm/isomorphic.browser.js"
  },
  "files": [
    "cjs",
    "esm",
    "portraits",
    "employees.json",
    "projects.json"
  ],
  "scripts": {
    "clean": "shx rm -rf .nyc_output coverage cjs esm",
    "lint": "npm run lint:typescript && npm run lint:javascript",
    "lint:typescript": "tslint -p tsconfig.json",
    "lint:javascript": "eslint test ./*.js",
    "build": "npm run build:cjs && npm run build:esm",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir cjs",
    "test": "npm run test:node && npm run test:browser && npm run lint",
    "test:node": "mocha",
    "test:browser": "karma start --single-run",
    "coverage": "npm run coverage:node && npm run coverage:browser",
    "coverage:node": "nyc node_modules/mocha/bin/mocha",
    "coverage:browser": "npm run test:browser -- --coverage",
    "upgrade": "npm-check -u && npm audit fix",
    "bump": "bump --tag --push --all",
    "release": "npm run upgrade && npm run clean && npm run build && npm test && npm run bump"
  },
  "devDependencies": {
    "@jsdevtools/eslint-config-modular": "^8.0.0",
    "@jsdevtools/karma-config": "^3.1.2",
    "@jsdevtools/tslint-modular": "^2.0.2",
    "@jsdevtools/version-bump-prompt": "^6.0.2",
    "@types/node": "^13.7.7",
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "karma": "^4.4.1",
    "karma-cli": "^2.0.0",
    "mocha": "^7.1.0",
    "npm-check": "^5.9.0",
    "nyc": "^15.0.0",
    "shx": "^0.3.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.2",
    "typescript-tslint-plugin": "^0.5.5"
  }
}
