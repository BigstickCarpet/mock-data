{
  "name": "static-mock-data",
  "version": "3.0.11",
  "description": "Static mock data for sample apps, demos, and POCs",
  "keywords": [
    "data",
    "mock",
    "sample",
    "samples",
    "users",
    "mock data",
    "sample data",
    "mock user data",
    "sample user data"
  ],
  "author": {
    "name": "James Messinger",
    "url": "http://jamesmessinger.com"
  },
  "license": "(MIT OR CC-BY-NC-SA-4.0)",
  "homepage": "https://github.com/BigstickCarpet/static-mock-data",
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib",
    "portraits",
    "employees.json",
    "projects.json"
  ],
  "scripts": {
    "lint": "jshint . --verbose && jscs . --verbose",
    "build": "npm run lint && npm run browserify",
    "browserify": "simplifyify lib/index.js --outfile dist/static-mock-data.js --standalone mock.data --debug --minify",
    "watch": "npm run browserify -- --watch",
    "test": "istanbul cover _mocha -- --bail --recursive tests && npm run karma",
    "mocha": "mocha --bail --recursive tests",
    "karma": "karma start --single-run",
    "upgrade": "ncu --upgradeAll && npm update && bower update",
    "bump": "bump --prompt --grep lib/index.js dist/* --tag --push --all",
    "release": "npm run upgrade && npm run build && npm test && npm run bump && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BigstickCarpet/static-mock-data.git"
  },
  "devDependencies": {
    "simplifyify": "^1.4.1",
    "bower": "^1.4.1",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "0.1.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.2.14",
    "istanbul": "^0.3.17",
    "coveralls": "^2.11.4",
    "mocha": "^2.2.5",
    "chai": "^3.2.0",
    "sinon": "^1.15.4",
    "jscs": "^2.1.0",
    "jshint": "^2.8.0",
    "npm-check-updates": "^2.2.0",
    "version-bump-prompt": "^1.5.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.0",
    "karma-sinon": "^1.0.4",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1"
  },
  "dependencies": {}
}