{
  "name": "static-mock-data",
  "version": "3.2.14",
  "description": "Static mock data for sample apps, demos, and POCs",
  "keywords": [
    "data",
    "mock",
    "sample",
    "samples",
    "users",
    "mock data",
    "sample data",
    "mock user data",
    "sample user data"
  ],
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "license": "(MIT OR CC-BY-NC-SA-4.0)",
  "homepage": "https://jsdevtools.org/static-mock-data",
  "repository": {
    "type": "git",
    "url": "https://github.com/JS-DevTools/static-mock-data.git"
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "browser": {
    "./src/node.ts": "./src/browser.ts",
    "./cjs/node.js": "./cjs/browser.js",
    "./esm/node.js": "./esm/browser.js"
  },
  "files": [
    "cjs",
    "esm",
    "portraits",
    "employees.json",
    "projects.json"
  ],
  "scripts": {
    "clean": "shx rm -rf .nyc_output coverage cjs esm",
    "lint": "npm run lint:typescript && npm run lint:javascript",
    "lint:typescript": "tslint -p tsconfig.json",
    "lint:javascript": "eslint test ./*.js",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir cjs",
    "test": "npm run test:node && npm run test:browser && npm run lint",
    "test:node": "mocha",
    "test:browser": "karma start --single-run=true",
    "coverage": "npm run coverage:node && npm run coverage:browser",
    "coverage:node": "nyc --reporter=text --reporter=lcov --report-dir coverage/node mocha",
    "coverage:browser": "npm run test:browser -- --coverage",
    "upgrade": "npm-check -u",
    "bump": "bump --prompt --tag --push --all",
    "release": "npm run upgrade && npm run build && npm test && npm run bump"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.6.1",
    "eslint-config-modular": "^6.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^4.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-host-environment": "^1.1.6",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^2.0.2",
    "karma-verbose-reporter": "0.0.6",
    "karma-webpack": "^3.0.5",
    "mocha": "^5.2.0",
    "npm-check": "^5.9.0",
    "nyc": "^13.1.0",
    "shx": "^0.3.2",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.0",
    "tslint-modular": "^1.1.7",
    "typescript": "^3.2.2",
    "typescript-tslint-plugin": "^0.3.0",
    "version-bump-prompt": "^4.2.1",
    "webpack": "^4.29.0"
  }
}
