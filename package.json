{
  "name": "static-mock-data",
  "version": "3.2.8",
  "description": "Static mock data for sample apps, demos, and POCs",
  "keywords": [
    "data",
    "mock",
    "sample",
    "samples",
    "users",
    "mock data",
    "sample data",
    "mock user data",
    "sample user data"
  ],
  "author": {
    "name": "James Messinger",
    "url": "http://bigstickcarpet.com"
  },
  "license": "(MIT OR CC-BY-NC-SA-4.0)",
  "homepage": "https://github.com/BigstickCarpet/static-mock-data",
  "repository": {
    "type": "git",
    "url": "https://github.com/BigstickCarpet/static-mock-data.git"
  },
  "main": "lib/node.js",
  "browser": "lib/browser.js",
  "files": [
    "dist/static-mock-data.js",
    "dist/static-mock-data.js.map",
    "dist/static-mock-data.min.js",
    "dist/static-mock-data.min.js.map",
    "lib",
    "portraits",
    "employees.json",
    "projects.json"
  ],
  "scripts": {
    "lint": "eslint lib test/fixtures test/specs --fix",
    "build": "simplifyify lib/browser.js --outfile dist/static-mock-data.js --standalone mock.data --bundle --debug --minify",
    "test": "npm run mocha && npm run karma && npm run lint",
    "cover": "istanbul cover _mocha --dir coverage/node && npm run build -- --test && npm run karma -- --cover",
    "mocha": "mocha",
    "karma": "karma start --single-run",
    "upgrade": "npm-check -u",
    "bump": "bump --prompt --grep dist/* --tag --push --all",
    "release": "npm run upgrade && npm run build && npm test && npm run bump && npm publish"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-jquery": "^2.0.0",
    "codacy-coverage": "^2.0.3",
    "coveralls": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-modular": "^4.1.1",
    "istanbul": "^0.4.5",
    "jquery": "^3.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-host-environment": "^1.1.2",
    "karma-ie-launcher": "^1.0.0",
    "karma-jquery-chai": "^0.1.3",
    "karma-mocha": "^1.2.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-verbose-reporter": "0.0.6",
    "mocha": "^4.1.0",
    "npm-check": "^5.5.2",
    "simplifyify": "^4.0.0",
    "version-bump-prompt": "^4.0.0"
  },
  "dependencies": {}
}