{
  "name": "@bigstickcarpet/mock-data",
  "version": "3.0.8",
  "description": "Mock data for sample apps, demos, and POCs",
  "keywords": [
    "mock data"
  ],
  "author": {
    "name": "James Messinger",
    "url": "http://jamesmessinger.com"
  },
  "license": "(MIT OR CC-BY-NC-SA-4.0)",
  "homepage": "https://github.com/BigstickCarpet/mock-data",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run lint && npm run browserify",
    "lint": "jshint . --verbose && jscs . --verbose",
    "browserify": "simplifyify lib/index.js --outfile dist/mock-data.js --standalone mock.data --debug --minify --test",
    "watch": "npm run browserify -- --watch",
    "test": "istanbul cover _mocha -- --bail --recursive tests && npm run karma",
    "mocha": "mocha --bail --recursive tests",
    "karma": "karma start --single-run",
    "upgrade": "npm-check-updates -u && npm update",
    "bump": "bump --prompt --tag --push --all",
    "release": "npm run upgrade && npm run build && npm test && npm run bump && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BigstickCarpet/mock-data.git"
  },
  "devDependencies": {
    "simplifyify": "^1.1.1",
    "karma": "^0.13.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "0.1.0",
    "karma-coverage": "^0.4.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.3",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sinon": "^1.0.4",
    "istanbul": "^0.3.17",
    "coveralls": "^2.11.3",
    "mocha": "^2.2.5",
    "chai": "^3.2.0",
    "sinon": "^1.15.4",
    "jscs": "^1.13.1",
    "jshint": "^2.8.0",
    "npm-check-updates": "^1.5.1",
    "version-bump-prompt": "^1.4.2",
    "jquery": "^2.1.4",
    "lodash": "^3.10.0"
  },
  "dependencies": {}
}