{
  "name": "static-mock-data",
  "version": "3.0.11",
  "description": "Static mock data for sample apps, demos, and POCs",
  "keywords": [
    "data",
    "mock",
    "sample",
    "samples",
    "users",
    "mock data",
    "sample data",
    "mock user data",
    "sample user data"
  ],
  "author": {
    "name": "James Messinger",
    "url": "http://jamesmessinger.com"
  },
  "license": "(MIT OR CC-BY-NC-SA-4.0)",
  "homepage": "https://github.com/BigstickCarpet/static-mock-data",
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib",
    "portraits",
    "employees.json",
    "projects.json"
  ],
  "scripts": {
    "lint": "eslint lib test --fix",
    "build": "npm run lint && npm run browserify",
    "browserify": "simplifyify lib/index.js --outfile dist/static-mock-data.js --standalone mock.data --bundle --debug --minify",
    "watch": "npm run browserify -- --watch",
    "test": "istanbul cover _mocha && npm run karma",
    "mocha": "mocha",
    "karma": "karma start --single-run",
    "upgrade": "npm-check -u && bower update",
    "bump": "bump --prompt --grep lib/index.js dist/* --tag --push --all",
    "release": "npm run upgrade && npm run build && npm test && npm run bump && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BigstickCarpet/static-mock-data.git"
  },
  "devDependencies": {
    "bower": "^1.4.1",
    "chai": "^3.2.0",
    "coveralls": "^2.11.4",
    "eslint": "^3.9.0",
    "eslint-config-modular": "^1.0.1",
    "istanbul": "^0.4.5",
    "jquery": "^3.1.1",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sinon": "^1.0.4",
    "lodash": "^4.16.4",
    "mocha": "^3.1.2",
    "npm-check": "^5.4.0",
    "simplifyify": "^2.0.4",
    "sinon": "^1.15.4",
    "version-bump-prompt": "^1.5.1"
  },
  "dependencies": {}
}
