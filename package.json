{
  "name": "static-mock-data",
  "version": "3.2.14",
  "description": "Static mock data for sample apps, demos, and POCs",
  "keywords": [
    "data",
    "mock",
    "sample",
    "samples",
    "users",
    "mock data",
    "sample data",
    "mock user data",
    "sample user data"
  ],
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "license": "(MIT OR CC-BY-NC-SA-4.0)",
  "homepage": "https://jsdevtools.org/static-mock-data",
  "repository": {
    "type": "git",
    "url": "https://github.com/JS-DevTools/static-mock-data.git"
  },
  "main": "cjs/src/node.js",
  "module": "esm/src/browser.js",
  "files": [
    "dist/static-mock-data.js",
    "dist/static-mock-data.js.map",
    "dist/static-mock-data.min.js",
    "dist/static-mock-data.min.js.map",
    "lib",
    "portraits",
    "employees.json",
    "projects.json"
  ],
  "scripts": {
    "lint": "npm run lint:typescript && npm run lint:javascript",
    "lint:typescript": "tslint -p tsconfig.json",
    "build": "npm run build-ts-cjs && npm run build-ts-esm && npm run build-rollup-prod",
    "lint:javascript": "eslint test ./*.js",
    "build-ts-esm": "tsc",
    "build-ts-cjs": "tsc --module commonjs --outDir cjs",
    "build-rollup-dev": "rollup -c rollup.config.dev.js",
    "build-rollup-prod": "rollup -c rollup.config.prod.js",
    "test": "npm run test:node && npm run test:browser && npm run lint",
    "test:node": "mocha",
    "test:browser": "karma start --single-run=true",
    "coverage": "npm run coverage:node && npm run coverage:browser",
    "coverage:node": "nyc --reporter=text --reporter=lcov --report-dir coverage/node mocha",
    "coverage:browser": "npm run test:browser -- --coverage",
    "upgrade": "npm-check -u",
    "bump": "bump --prompt --grep dist/* --tag --push --all",
    "release": "npm run upgrade && npm run build && npm test && npm run bump"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "chai-jquery": "^2.0.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.6.1",
    "eslint-config-modular": "^6.0.0",
    "jquery": "^3.3.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^1.1.2",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-host-environment": "^1.1.6",
    "karma-ie-launcher": "^1.0.0",
    "karma-jquery-chai": "^0.1.3",
    "karma-mocha": "^1.2.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^2.0.2",
    "karma-verbose-reporter": "0.0.6",
    "mocha": "^5.2.0",
    "npm-check": "^5.9.0",
    "nyc": "^13.1.0",
    "rollup": "^1.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^3.0.0",
    "tslint": "^5.12.0",
    "tslint-modular": "^1.1.7",
    "typescript": "^3.2.2",
    "typescript-tslint-plugin": "^0.2.1",
    "version-bump-prompt": "^4.2.1"
  },
  "dependencies": {}
}
